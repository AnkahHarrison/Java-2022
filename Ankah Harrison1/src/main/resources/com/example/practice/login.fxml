<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="251.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.practice.HelloController">
   <children>
      <Label layoutX="26.0" layoutY="62.0" prefHeight="29.0" prefWidth="78.0" text="Username">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Label layoutX="26.0" layoutY="117.0" text="Password">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <TextField fx:id="txtUsername" layoutX="104.0" layoutY="64.0" promptText="Username" />
      <Button fx:id="btnLogin" layoutX="87.0" layoutY="184.0" mnemonicParsing="false" onAction="#onLogin" text="Login" />
      <Label fx:id="lblLogin" layoutX="98.0" layoutY="148.0" prefHeight="17.0" prefWidth="156.0" />
      <Button fx:id="btnCancel" layoutX="179.0" layoutY="184.0" mnemonicParsing="false" onAction="#onCancel" text="Cancel" />
      <PasswordField fx:id="txtPassword" layoutX="104.0" layoutY="115.0" promptText="Password" />
   </children>
</AnchorPane>
