<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="328.0" prefWidth="361.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.practical.HelloController">
   <children>
      <TextField fx:id="txtUsername" layoutX="106.0" layoutY="69.0" promptText="Username" />
      <Label fx:id="lblDisplay" layoutX="108.0" layoutY="164.0" prefHeight="17.0" prefWidth="149.0" />
      <Label fx:id="lblRegister" alignment="CENTER" layoutX="137.0" layoutY="195.0" prefHeight="17.0" prefWidth="87.0" text="Register" textAlignment="RIGHT" />
      <PasswordField fx:id="txtPassword" layoutX="106.0" layoutY="126.0" promptText="password" />
      <Button fx:id="btnLogin" layoutX="106.0" layoutY="224.0" mnemonicParsing="false" onAction="#onLogin" text="Login" />
      <Button fx:id="btnCancel" layoutX="189.0" layoutY="224.0" mnemonicParsing="false" onAction="#onLogout" text="Cancel" />
   </children>
</AnchorPane>
